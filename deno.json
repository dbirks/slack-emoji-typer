{
  "name": "slack-emoji-typer",
  "version": "0.2.0",
  "exports": "./main.ts",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "nodeModulesDir": "auto",
  "tasks": {
    "dev": "deno run --allow-net --allow-env main.ts",
    "fmt": "deno fmt",
    "build": "deno compile --allow-net --allow-env main.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "ink": "npm:ink@^6.1.0",
    "react": "npm:react@^19.1.1",
    "@types/react": "npm:@types/react@^19.1.9"
  }
}
