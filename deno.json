{
  "name": "slack-emoji-typer",
  "version": "1.0.0",
  "exports": "./main.ts",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "nodeModulesDir": "auto",
  "tasks": {
    "dev": "deno run --allow-net --allow-env --allow-read main.ts",
    "fmt": "deno fmt",
    "build": "deno compile --allow-net --allow-env --allow-read=$HOME/.netrc main.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "ink": "npm:ink@3",
    "ink-use-stdout-dimensions": "npm:ink-use-stdout-dimensions@1",
    "react": "npm:react@18",
    "@types/react": "npm:@types/react@18"
  }
}
